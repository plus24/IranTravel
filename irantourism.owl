  <owl:NamedIndividual rdf:about="http://www.masoudehsani.com/semanticweb/irantourism.owl#Marivan">
    <rdf:type rdf:resource="http://www.masoudehsani.com/semanticweb/irantourism.owl#City"/>
    <locatedinprovince rdf:resource="http://www.masoudehsani.com/semanticweb/irantourism.owl#Kurdistan_Province"/>
    <id rdf:datatype="http://www.w3.org/2001/XMLSchema#decimal">155</id>
    <latitude rdf:datatype="http://www.w3.org/2001/XMLSchema#decimal">35.5269000000000000</latitude>
    <longitude rdf:datatype="http://www.w3.org/2001/XMLSchema#decimal">46.1764000000000000</longitude>
    <arabicname rdf:datatype="http://www.w3.org/2001/XMLSchema#string">مريوان</arabicname>
