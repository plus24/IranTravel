  <owl:NamedIndividual rdf:about="http://www.masoudehsani.com/semanticweb/irantourism.owl#Bandar_Abbas">
    <rdf:type rdf:resource="http://www.masoudehsani.com/semanticweb/irantourism.owl#City"/>
