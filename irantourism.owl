  <owl:NamedIndividual rdf:about="http://www.masoudehsani.com/semanticweb/irantourism.owl#Kelardasht">
    <rdf:type rdf:resource="http://www.masoudehsani.com/semanticweb/irantourism.owl#City"/>
    <locatedinprovince rdf:resource="http://www.masoudehsani.com/semanticweb/irantourism.owl#Mazandaran_Province"/>
