    <latitude rdf:datatype="http://www.w3.org/2001/XMLSchema#decimal">37.0771869999999900</latitude>
    <longitude rdf:datatype="http://www.w3.org/2001/XMLSchema#decimal">49.2511994999999840</longitude>
