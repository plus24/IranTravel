  <owl:NamedIndividual rdf:about="http://www.masoudehsani.com/semanticweb/irantourism.owl#Pahlavan_Pour">
    <rdf:type rdf:resource="http://www.masoudehsani.com/semanticweb/irantourism.owl#Garden_Hotel"/>
    <locatedinprovince rdf:resource="http://www.masoudehsani.com/semanticweb/irantourism.owl#Yazd_Province"/>
    <locatedincity rdf:resource="http://www.masoudehsani.com/semanticweb/irantourism.owl#Yazd"/>
    <id rdf:datatype="http://www.w3.org/2001/XMLSchema#decimal">7851</id>
