  <owl:NamedIndividual rdf:about="http://www.masoudehsani.com/semanticweb/irantourism.owl#Shahedan">
    <rdf:type rdf:resource="http://www.masoudehsani.com/semanticweb/irantourism.owl#Eco_Lodge"/>
